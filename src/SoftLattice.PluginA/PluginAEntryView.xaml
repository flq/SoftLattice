<UserControl x:Class="SoftLattice.PluginA.PluginAEntryView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Height="300" Width="300"  AutomationProperties.AutomationId="PluginAEntryView">
    <StackPanel>
      <TextBox AutomationProperties.AutomationId="StyledTextbox" Style="{StaticResource PluginA.SetText}"></TextBox>
      <TextBox AutomationProperties.AutomationId="StyledTextboxFromLookup" Style="{StaticResource PluginA.SetTextFromLookup}"></TextBox>
    </StackPanel>
</UserControl>